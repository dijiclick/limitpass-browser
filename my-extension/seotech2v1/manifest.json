{
  "manifest_version": 3,
  "name": "SeoTech Discord",
  "version": "1.6.0",
  "description": "Access your Seotech tools easily on Google Chrome.",
  "key": "c2VvVGVjaGRldnRlY2hzUklLSQ==",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "webRequest",
    "contextMenus",
    "cookies",
    "declarativeNetRequest",
    "debugger",
    "declarativeNetRequestWithHostAccess",
    "management",
     "windows",
     "proxy",
    "webRequestAuthProvider",
    "declarativeNetRequestFeedback",
   "webNavigation",
   "scripting"
  ],
  
  "icons": {
    "16": "assets/icons/icon_16.png",
    "48": "assets/icons/icon_48.png",
    "128": "assets/icons/icon_128.png"
  },
  "background": {
    "service_worker": "./content/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content/jquery.js","./content/index.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
 "host_permissions": ["<all_urls>"]
}